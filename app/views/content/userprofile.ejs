<% if (profile) { %>
<div class="row m-b-lg m-t-lg">
    <div class="col-md-6">
        <div class="profile-image">
            <img src="/img/128_128.jpg" class="img-circle circle-border m-b-md" alt="profile">
        </div>
        <div class="profile-info">
            <div class="">
                <div>
                    <h2 class="no-margins"><%= profile.fullname %></h2>
                    <h4><%= profile.jobTitle %></h4>
                </div>
            </div>
        </div>
    </div>
</div>
<% } %>